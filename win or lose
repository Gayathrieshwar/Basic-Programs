#include <stdio.h>

int main() {
    int n,i;
    printf("How many number you want to enter?");
    scanf("%d",&n);
    
    int a[n];
    
    printf("Enter the nums:\n");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    
    int max=a[0];
    
    for(i=1;i<n;i++){
        if(a[i]>max)
        max=a[i];
    }
    if(max%2==0){
         printf("WON"); 
    }else
    printf("LOST");
 return 0;
}
